{
  "name": "dicom-threejs",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "rm -rf .cache/ && parcel src/index.html --no-source-maps -d public/",
    "build": "rm -rf build/ && parcel build src/index.html --no-source-maps -d build/ && cp -R public/resources build/resources"
  },
  "dependencies": {
    "three": "^0.97.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "json5": "^2.1.0",
    "parcel-bundler": "^1.10.1"
  }
}
